#!/usr/bin/env bash

echo "Create ~/Applications"
mkdir -p ~/Applications/

echo "Shadowsocks: setup rules"
./Shadowsocks/init

echo "Alacritty"
./alacritty/init

echo "tmux"
./tmux/init

echo "fzf"
$(brew --prefix)/opt/fzf/install

echo "Ruby"
./ruby/init

echo "Go"
mkdir -p ~/go

echo "neovim"
./neovim/init

echo "Karabiner"
./Karabiner/init

echo "Snipaste"
./snipaste/init
